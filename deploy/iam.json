    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ArchiveFile",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObject",
                "s3:GetObjectAcl",
                "s3:DeleteObject"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::s3-bucket-to-sftp-server-files/*",
                "arn:aws:s3:::s3-bucket-to-sftp-server-files/"
            ]
        }
    ]
}